  <!-- EN: drowsy -->
  <NeedRest.stages.drowsy.label>senność</NeedRest.stages.drowsy.label>
  <!-- EN: I haven't slept in a while. It's hard to keep focus. -->
  <NeedRest.stages.drowsy.description>Od jakiegoś czasu nie śpię. Trudno jest mi się skupić.</NeedRest.stages.drowsy.description>
  <!-- EN: tired -->
