<LanguageData>
  
  <!-- EN: advanced gravtech -->
  <AdvancedGravtech.label>zaawansowany grawtech</AdvancedGravtech.label>
  <!-- EN: Build advanced gravship parts. -->
  <AdvancedGravtech.description>Buduj zaawansowane części grawstatku.</AdvancedGravtech.description>
  <!-- EN:
    <li>subject->advanced gravtech</li>
