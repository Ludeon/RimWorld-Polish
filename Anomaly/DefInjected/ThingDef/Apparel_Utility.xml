  <Apparel_ShardPsychicShockLance.comps.CompApparelReloadable.cooldownGerund>odnawianie</Apparel_ShardPsychicShockLance.comps.CompApparelReloadable.cooldownGerund>
  <!-- EN: shard shock lance -->
  <Apparel_ShardPsychicShockLance.verbs.shard_shock_lance.label>odłamkowa lanca szoku</Apparel_ShardPsychicShockLance.verbs.shard_shock_lance.label>
